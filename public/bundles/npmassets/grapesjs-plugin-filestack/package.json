{
  "name": "grapesjs-plugin-filestack",
  "version": "0.1.1",
  "description": "Enable Filestack uploader inside the Asset Manager",
  "main": "dist/grapesjs-plugin-filestack.min.js",
  "scripts": {
    "lint": "eslint src",
    "build": "npm run v:patch && webpack --env.production",
